pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
          stage('build') {
            steps {
                echo 'build'
            }
        }
          stage('Test') {
            steps {
                echo 'test'
            }
        }
          stage('Deploy') {
            steps {
                echo 'deploy'
            }
        }
    }
}

